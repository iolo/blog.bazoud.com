        <a target="_blank" style="cursor:pointer;" href="<%= @Site.config.feeds.comments %>"><img alt="Comments Feed" title="Comments Feed" src="/images/rss.png" /></a>
      </div>
    </div>
    <div class="content">
      <div class="row">
        <div class="span12">
          <%- @content %>
        </div>
      </div>
    </div>
  </div>
